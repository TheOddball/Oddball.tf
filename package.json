{
    "name": "oddball.tf",
    "version": "1.2.0",
    "scripts": {
        "test": "parcel ./src/Oddball.tf/index.pug",
        "test-elise": "parcel ./src/Elise.tf/index.pug",
        "test-SSS": "parcel ./src/SuperSatanSon.com/index.pug",
        "test-bdino": "parcel ./src/bdino.info/index.pug",
        "build-oddball": "parcel build ./src/Oddball.tf/index.pug --public-url ./",
        "build-SSS": "parcel build ./src/SuperSatanSon.com/index.pug --public-url ./",
        "build-bdino": "parcel build ./src/bdino.info/index.pug",
        "build-elise": "parcel build ./src/Elise.tf/index.pug --public-url ./",
        "build-all-win": "del /Q dist && npm run build-oddball && del /Q dist && npm run build-SSS && del /Q dist && npm run build-elise && del /Q dist",
        "build-all-linux": "rm -rf dist && npm run build-oddball && rm -rf dist && npm run build-SSS && rm -rf dist && npm run build-elise && rm -rf dist"
    },
    "dependencies": {
        "@parcel/packager-raw-url": "^2.7.0",
        "@parcel/packager-xml": "^2.7.0",
        "@parcel/transformer-image": "^2.7.0",
        "@parcel/transformer-pug": "^2.7.0",
        "@parcel/transformer-stylus": "^2.7.0",
        "@parcel/transformer-webmanifest": "^2.7.0",
        "@parcel/transformer-xml": "^2.7.0",
        "@parcel/validator-eslint": "^2.7.0",
        "jquery": "^3.6.1",
        "parcel": "^2.7.0",
        "parcel-reporter-static-files-copy": "^1.4.0",
        "pug": "^3.0.0",
        "stylus": "^0.59.0"
    }
}